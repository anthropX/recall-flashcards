{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": [
    "react-app",
    "plugin:react/recommended",
    "airbnb",
    "plugin:prettier/recommended",
    "prettier/react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "react/jsx-filename-extension": ["error", { "extensions": [".js"] }],
    "no-shadow": "off",
    "no-use-before-define": "off"
  }
}
